        <TextBox x:Name="Код_группы1" HorizontalAlignment="Left" Margin="1120,153,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233"/>
        <TextBox x:Name="Название_группы1" HorizontalAlignment="Left" Margin="1120,191,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="233"/>
