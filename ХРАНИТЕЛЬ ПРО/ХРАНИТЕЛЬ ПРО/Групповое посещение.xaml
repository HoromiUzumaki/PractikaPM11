        <Label Content="Код группы * :" HorizontalAlignment="Left" Margin="966,145,0,0" VerticalAlignment="Top"/>
        <Label Content="Название группы * :" HorizontalAlignment="Left" Margin="966,186,0,0" VerticalAlignment="Top"/>
        <Label Content="Пользователи * :" HorizontalAlignment="Left" Margin="966,227,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="dat1" HorizontalAlignment="Left" Margin="945,388,0,0" VerticalAlignment="Top" Width="162"/>
        <DatePicker x:Name="dat2" HorizontalAlignment="Left" Margin="1199,388,0,0" VerticalAlignment="Top" Width="177"/>
        <DatePicker x:Name="datebirth" HorizontalAlignment="Left" Margin="434,218,0,0" VerticalAlignment="Top" Width="102"/>
